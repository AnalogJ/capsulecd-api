{
  "name": "connect",
  "customName": false,
  "customRole": false,
  "handler": "connect/handler.handler",
  "timeout": 6,
  "memorySize": 128,
  "custom": {
    "excludePatterns": [],
    "envVars": []
  },
  "endpoints": [
    {
      "path": "connect/{serviceType}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplate}",
      "responses": {
        ".*400.*": {
          "statusCode": "400",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": "$${apiFailureResponseTemplate}"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": "$${apiSuccessResponseTemplate}"
        }
      }
    }
  ],
  "events": [],
  "custom": {
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": ["Content-Type", "X-Amz-Date", "X-CapsuleCD-Auth", "X-Api-Key"]
    }
  }
}