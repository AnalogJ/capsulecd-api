{
  "name": "hook",
  "customName": false,
  "customRole": false,
  "handler": "hook/handler.handler",
  "runtime": "nodejs",
  "timeout": 10,
  "memorySize": 128,
  "custom": {
    "excludePatterns": [],
    "envVars": []
  },
  "endpoints": [
    {
      "path": "hook/{serviceType}/{orgId}/{repoId}",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplate}",
      "responses": {
        ".*400.*": {
          "statusCode": "400",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": "$${apiFailureResponseTemplate}"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": "$${apiSuccessResponseTemplate}"
        }
      }
    }
  ],
  "events": []
}