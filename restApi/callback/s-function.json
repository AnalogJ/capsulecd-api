{
  "name": "callback",
  "customName": false,
  "customRole": false,
  "handler": "callback/handler.handler",
  "timeout": 6,
  "memorySize": 128,
  "custom": {
    "excludePatterns": [],
    "envVars": []
  },
  "endpoints": [
    {
      "path": "callback/{serviceType}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "apiKeyRequired": false,
      "requestParameters": {
        "integration.request.querystring.code": "method.request.querystring.code"
      },
      "requestTemplates": "$${apiRequestTemplate}",
      "responses": {
        ".*400.*": {
          "statusCode": "400",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": "$${apiFailureResponseTemplate}"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": "$${apiSuccessResponseTemplate}"
        }
      }
    }
  ],
  "events": []
}