{
  "name": "user",
  "runtime": "nodejs",
  "description": "Serverless Lambda function for project: capsulecd-api",
  "customName": false,
  "customRole": false,
  "handler": "user/handler.handler",
  "timeout": 6,
  "memorySize": 128,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "envVars": [],
    "cors": "$${corsTemplate}"
  },
  "endpoints": [
    {
      "path": "user",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${apiRequestTemplate}",
      "responses": {
        ".*400.*": {
          "statusCode": "400",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": "$${apiFailureResponseTemplate}"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {},
          "responseTemplates": "$${apiSuccessResponseTemplate}"
        }
      }
    }
  ],
  "events": [],
  "environment": "$${environmentTemplate}",
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}